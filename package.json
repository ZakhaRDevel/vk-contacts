{
  "name": "vk-contacts",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:static": "ng build --configuration=static",
    "watch": "ng build --watch --configuration development",
    "serve:ssr:vk-contacts": "node dist/vk-contacts/server/server.mjs",
    "deploy": "npm run build:static && npx angular-cli-ghpages --dir=dist/vk-contacts/browser"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.1.0",
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/platform-server": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@angular/ssr": "^20.1.0",
    "embla-carousel-angular": "^20.0.0",
    "express": "^5.1.0",
    "gsap": "^3.13.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.1.0",
    "@angular/cli": "^20.1.0",
    "@angular/compiler-cli": "^20.1.0",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.19",
    "angular-cli-ghpages": "^2.0.3",
    "typescript": "~5.8.2"
  }
}
